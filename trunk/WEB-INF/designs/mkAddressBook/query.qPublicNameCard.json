				"f_config":
				{
					"customscript":"var mAddressBook = Ext.getCmp('m-mkAddressBook');\nif(Ext.isDefined(mAddressBook)){\n\tmAddressBook.items.items[0].moduleTree.addListener('beforenodedrop', function(e) {\n\t\tif(Ext.isArray(e.data.selections)) {\n\t\t\tvar r;\n\t\t\tfor(var i = 0; i < e.data.selections.length; i++) {\n\t\t\t\tr = e.data.selections[i];\n\t\t\t\tvar targetParams = e.target.attributes.params;\n\t\t\t\tif (targetParams && targetParams.gpByTagS && targetParams.gpByTagS != '') {\n\t\t\t\t\tAddressBookAppDirect.tagAddressBook(parseInt(r.get('ID')), targetParams.gpByTagS, 't_mk_app_namecard', function(result, event) {\n\t\t\t\t\t\tif (result && result.success) {\n\t\t\t\t\t\t\te.target.parentNode.reload();\n\t\t\t\t\t\t} else {\n\t\t\t\t\t\t\tExt.Msg.alert('信息提示', r.get('F_NAME') + '已打过' + targetParams.gpByTagS + '标签！');\n\t\t\t\t\t\t}\n\t\t\t\t\t});\n\t\t\t\t} else \n\t\t\t\t\treturn false;\n\t\t\t}\n\t\t\treturn true;\n\t\t}\n\t});\n}",
					"enableDragDrop":false
				},
